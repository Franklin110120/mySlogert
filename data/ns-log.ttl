#################################################################
#    Classes
#################################################################

###  https://w3id.org/sepses/ns/log#Action
log:Action rdf:type owl:Class ;
           rdfs:subClassOf log:Tag ;
           rdfs:comment "The primary type of action that was undertaken as part of the event. The status or result of the action should be detailed in the status field." .


###  https://w3id.org/sepses/ns/log#Address
log:Address rdf:type owl:Class ;
            rdfs:subClassOf log:Parameter ;
            rdfs:comment "Logical address in a network, which consists of hostname, IPv4, IPv6, and port" ;
            rdfs:label "Network Address" .


###  https://w3id.org/sepses/ns/log#App
log:App rdf:type owl:Class ;
        rdfs:subClassOf log:Parameter ;
        rdfs:comment "Application" ;
        rdfs:label "Application" .


###  https://w3id.org/sepses/ns/log#Domain
log:Domain rdf:type owl:Class ;
           rdfs:subClassOf log:Tag ;
           rdfs:comment "The environment or domain of the event. Typical event domains include network (net), operating system (os), and application (app)." .


###  https://w3id.org/sepses/ns/log#Event
log:Event rdf:type owl:Class ;
          rdfs:comment "A representation of a single logline in a log file." .


###  https://w3id.org/sepses/ns/log#File
log:File rdf:type owl:Class ;
         rdfs:subClassOf log:Parameter ;
         rdfs:comment "File information" ;
         rdfs:label "File" .


###  https://w3id.org/sepses/ns/log#Format
log:Format rdf:type owl:Class ;
           rdfs:comment "The format used to represent loglines for a certain type of log source, e.g., unix format that is used in Ubuntu's Kernel, Auth, and System log files." .


###  https://w3id.org/sepses/ns/log#Host
log:Host rdf:type owl:Class ;
         rdfs:subClassOf log:Parameter .


###  https://w3id.org/sepses/ns/log#IP
log:IP rdf:type owl:Class ;
       rdfs:subClassOf log:Parameter .


###  https://w3id.org/sepses/ns/log#IPv4
log:IPv4 rdf:type owl:Class ;
         rdfs:subClassOf log:IP .


###  https://w3id.org/sepses/ns/log#IPv6
log:IPv6 rdf:type owl:Class ;
         rdfs:subClassOf log:IP .


###  https://w3id.org/sepses/ns/log#Object
log:Object rdf:type owl:Class ;
           rdfs:subClassOf log:Tag ;
           rdfs:comment "The type of object that is targeted or otherwise affected by the event" .


###  https://w3id.org/sepses/ns/log#Parameter
log:Parameter rdf:type owl:Class ;
              rdfs:comment "The superclass of various classes that represent field dictionary items. Most of them are from CEE standard, while several classes are added (e.g., URL)." .


###  https://w3id.org/sepses/ns/log#Port
log:Port rdf:type owl:Class ;
         rdfs:subClassOf log:Parameter .


###  https://w3id.org/sepses/ns/log#Proc
log:Proc rdf:type owl:Class ;
         rdfs:subClassOf log:Parameter ;
         rdfs:comment "Process" ;
         rdfs:label "Process" .


###  https://w3id.org/sepses/ns/log#Service
log:Service rdf:type owl:Class ;
            rdfs:subClassOf log:Tag ;
            rdfs:comment "The service the event involves. The service field value provides context to the event action or more precision to the event domain." .


###  https://w3id.org/sepses/ns/log#Source
log:Source rdf:type owl:Class ;
           rdfs:comment """The class represent a single log source. It may represent an aggregation of logs from different applications (e.g., SysLog) or aggregation of several log files from different devices. 

An instance of this class, however, should only represent log data from a single source type (e.g., Syslog), so it can be processed consistently.""" .


###  https://w3id.org/sepses/ns/log#SourceType
log:SourceType rdf:type owl:Class ;
               rdfs:comment "The type of a log source, e.g., apache-access, kernel, syslog, audit, etc." .


###  https://w3id.org/sepses/ns/log#Status
log:Status rdf:type owl:Class ;
           rdfs:subClassOf log:Tag ;
           rdfs:comment "The end result or status of the event action identified by the action field." ;
           rdfs:label "Status" .


###  https://w3id.org/sepses/ns/log#Subject
log:Subject rdf:type owl:Class ;
            rdfs:subClassOf log:Tag ;
            rdfs:comment "The type of object that initiated or started the event action identified by the action field." .


###  https://w3id.org/sepses/ns/log#Syslog
log:Syslog rdf:type owl:Class ;
           rdfs:subClassOf log:Parameter ;
           rdfs:comment "Syslog compatibility" ;
           rdfs:label "Syslog" .


###  https://w3id.org/sepses/ns/log#Tag
log:Tag rdf:type owl:Class ;
        rdfs:comment "This class represent a generic annotation class that can be attached to an Event for Event classification." .


###  https://w3id.org/sepses/ns/log#URL
log:URL rdf:type owl:Class ;
        rdfs:subClassOf log:Parameter ;
        rdfs:comment "Uniform Resource Locator (URL)" ;
        rdfs:label "URL" .


###  https://w3id.org/sepses/ns/log#User
log:User rdf:type owl:Class ;
         rdfs:subClassOf log:Parameter ;
         rdfs:comment "User account" ;
         rdfs:label "User Account" .


