@prefix : <https://w3id.org/sepses/ns/log#> .
@prefix lid: <https://w3id.org/sepses/id/log/> .
@prefix log: <https://w3id.org/sepses/ns/log#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://w3id.org/sepses/ns/log#> .

<https://w3id.org/sepses/ns/log#> rdf:type owl:Ontology ;
                                   owl:versionIRI <https://w3id.org/sepses/ns/log/1.0.0> ;
                                   <http://purl.org/dc/elements/1.1/creator> "Andreas Ekelhart" ,
                                                                             "Elmar Kiesling" ,
                                                                             "Fajar J. Ekaputra" ;
                                   <http://purl.org/dc/elements/1.1/description> """An ontology to represent log events. 
This ontology is developed based on the CEE core profile (https://cee.mitre.org/language/1.0-beta1/core-profile.html)"""@en ;
                                   <http://purl.org/dc/elements/1.1/rights> "http://creativecommons.org/licenses/by/4.0/" ;
                                   <http://purl.org/dc/elements/1.1/title> "The SLOGERT (Semantic LOG ExtRaction Template) log event ontology." ;
                                   <http://purl.org/vocab/vann/preferredNamespacePrefix> "log" .

#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/sepses/ns/log#hasActionTag
log:hasActionTag rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf log:hasEventTag ;
                 rdfs:range log:Action .


###  https://w3id.org/sepses/ns/log#hasAddress
log:hasAddress rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf log:hasParameter ;
               rdfs:range log:Address ;
               rdfs:comment "A relation between an Event and a Network Address (a combination of log:host, log:ipv4, log:ipv6, an log:port)" .


###  https://w3id.org/sepses/ns/log#hasAddressHost
log:hasAddressHost rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   rdfs:domain log:Address ;
                   rdfs:range log:Host .


###  https://w3id.org/sepses/ns/log#hasAddressIp
log:hasAddressIp rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:domain log:Address ;
                 rdfs:range log:IP .


###  https://w3id.org/sepses/ns/log#hasAddressPort
log:hasAddressPort rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   rdfs:domain log:Address ;
                   rdfs:range log:Port .


###  https://w3id.org/sepses/ns/log#hasApp
log:hasApp rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf log:hasParameter ;
           rdfs:range log:App ;
           rdfs:comment "Relation between an Event and an Application that it contained (log:App)" .


###  https://w3id.org/sepses/ns/log#hasDomainTag
log:hasDomainTag rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf log:hasEventTag ;
                 rdfs:range log:Domain .


###  https://w3id.org/sepses/ns/log#hasEventTag
log:hasEventTag rdf:type owl:ObjectProperty ;
                rdfs:domain log:Event ;
                rdfs:range log:Tag ;
                rdfs:comment "Generic relation between an Event and its annotations (Tag)" .


###  https://w3id.org/sepses/ns/log#hasFile
log:hasFile rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf log:hasParameter ;
            rdfs:range log:File ;
            rdfs:comment "Relation between an Event and a File that it contained (log:File)" .


###  https://w3id.org/sepses/ns/log#hasFormat
log:hasFormat rdf:type owl:ObjectProperty ;
              rdfs:domain log:SourceType ;
              rdfs:range log:Format ;
              rdfs:comment "A log source type bound to a specific format." .


###  https://w3id.org/sepses/ns/log#hasHost
log:hasHost rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf log:hasParameter ;
            rdfs:range log:Host .


###  https://w3id.org/sepses/ns/log#hasIP
log:hasIP rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf log:hasParameter ;
          rdfs:range log:IP .


###  https://w3id.org/sepses/ns/log#hasIPv4
log:hasIPv4 rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf log:hasIP ;
            rdfs:range log:IPv4 .


###  https://w3id.org/sepses/ns/log#hasIPv6
log:hasIPv6 rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf log:hasIP ;
            rdfs:range log:IPv6 .


###  https://w3id.org/sepses/ns/log#hasLogSource
log:hasLogSource rdf:type owl:ObjectProperty ;
                 rdfs:domain log:Event ;
                 rdfs:range log:Source ;
                 rdfs:comment "Links betwee an event and the data source of the event (e.g., log file or log stream)." .


###  https://w3id.org/sepses/ns/log#hasNetworkDestination
log:hasNetworkDestination rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf log:hasAddress ;
                          rdfs:comment "Links between and Event and its Network destination" .


###  https://w3id.org/sepses/ns/log#hasNetworkSource
log:hasNetworkSource rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf log:hasAddress ;
                     rdfs:comment "Links between and Event and its Network source" .


###  https://w3id.org/sepses/ns/log#hasObjectTag
log:hasObjectTag rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf log:hasEventTag ;
                 rdfs:range log:Object .


###  https://w3id.org/sepses/ns/log#hasParameter
log:hasParameter rdf:type owl:ObjectProperty ;
                 rdfs:domain log:Event ;
                 rdfs:range log:Parameter ;
                 rdfs:comment "Relation between an Event and Field Objects" .


###  https://w3id.org/sepses/ns/log#hasPort
log:hasPort rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf log:hasParameter .


###  https://w3id.org/sepses/ns/log#hasProc
log:hasProc rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf log:hasParameter ;
            rdfs:range log:Proc ;
            rdfs:comment "Relation between an Event and a Process (log:Proc)" .


###  https://w3id.org/sepses/ns/log#hasServiceTag
log:hasServiceTag rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf log:hasEventTag ;
                  rdfs:range log:Service .


###  https://w3id.org/sepses/ns/log#hasSourceHost
log:hasSourceHost rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf log:hasHost ;
                  rdfs:domain log:Address ;
                  rdfs:range log:Host .


###  https://w3id.org/sepses/ns/log#hasSourceType
log:hasSourceType rdf:type owl:ObjectProperty ;
                  rdfs:domain log:Source ;
                  rdfs:range log:SourceType ;
                  rdfs:comment "links between a log source and its source type (e.g., apache-access, kernel log)" .


###  https://w3id.org/sepses/ns/log#hasStatusTag
log:hasStatusTag rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf log:hasEventTag ;
                 rdfs:range log:Subject .


###  https://w3id.org/sepses/ns/log#hasSubjectTag
log:hasSubjectTag rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf log:hasEventTag ;
                  rdfs:range log:Subject .


###  https://w3id.org/sepses/ns/log#hasSyslog
log:hasSyslog rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf log:hasParameter ;
              rdfs:range log:Syslog ;
              rdfs:comment "Relation between an Event and its Syslog information (log:Syslog)" .


###  https://w3id.org/sepses/ns/log#hasURL
log:hasURL rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf log:hasParameter ;
           rdfs:range log:URL ;
           rdfs:comment "URL pointed out in a LogEvent" .


###  https://w3id.org/sepses/ns/log#hasUser
log:hasUser rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf log:hasParameter ;
            rdfs:range log:User ;
            rdfs:comment """Relation between an Event and a User account that in contained (log:
            )""" .


